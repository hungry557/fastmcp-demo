{
  "name": "@hungry557/fastmcp-demo",
  "version": "1.0.3",
  "description": "一个使用FastMCP框架的MCP服务器示例",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "fastmcp-demo": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "npx fastmcp dev src/index.ts",
    "inspect": "npx fastmcp inspect src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --stdio",
    "start:sse": "node dist/index.js --port 8080",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "fastmcp",
    "ai",
    "claude",
    "model-context-protocol",
    "sse"
  ],
  "author": {
    "name": "hungry557",
    "email": "2113826604@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hungry557/fastmcp-demo.git"
  },
  "bugs": {
    "url": "https://github.com/hungry557/fastmcp-demo/issues"
  },
  "homepage": "https://github.com/hungry557/fastmcp-demo#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "fastmcp": "^1.21.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
